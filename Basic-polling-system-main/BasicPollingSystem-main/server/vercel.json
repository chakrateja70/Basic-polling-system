{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "Access-Control-Allow-Origin": "https://basic-polling-system-client.vercel.app",
                "Access-Control-Allow-Credentials": "true"
              },
            "dest": "/"
        }
    ]
}